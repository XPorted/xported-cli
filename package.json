{
	"name": "xported-cli",
	"version": "0.1.0",
	"description": "> A fast command-line tool for local and remote file management.",
	"main": "dist/index.js",
	"bin": {
		"xported": "dist/index.js"
	},
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"start": "node dist/index.js",
		"build": "tsc",
		"build:watch": "tsc --watch",
		"build:dev": "tsc --watch --preserveWatchOutput",
		"lint": "eslint src/**/*.ts",
		"format": "prettier --write src/**/*.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/XPorted/xported-cli.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "module",
	"bugs": {
		"url": "https://github.com/XPorted/xported-cli/issues"
	},
	"homepage": "https://github.com/XPorted/xported-cli#readme",
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^22.14.1",
		"@typescript-eslint/eslint-plugin": "^8.30.1",
		"@typescript-eslint/parser": "^8.30.1",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"jest": "^29.7.0",
		"prettier": "^3.5.3",
		"ts-jest": "^29.3.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	}
}
